package br.com.reportmanager.report;

public class Report {

	private Database database;

	private FTP ftp;

	private Parameter parameter;

	private TypeFormat typeFormat;

	public Report(Database database, FTP ftp, Parameter parameter) {
		this.database = database;
		this.ftp = ftp;
		this.parameter = parameter;
	}

	public Report() {

	}

	public Database getDatabase() {
		return database;
	}

	public void setDatabase(Database database) {
		this.database = database;
	}

	public FTP getFtp() {
		return ftp;
	}

	public void setFtp(FTP ftp) {
		this.ftp = ftp;
	}

	public Parameter getParameter() {
		return parameter;
	}

	public void setParameter(Parameter parameter) {
		this.parameter = parameter;
	}

	public TypeFormat getTypeFormat() {
		return typeFormat;
	}

	public void setTypeFormat(TypeFormat typeFormat) {
		this.typeFormat = typeFormat;
	}

}
